{
  "name": "epk",
  "version": "0.0.1",
  "description": "JS testing tool for a world without problems",
  "main": "dist/index.js",
  "browser": "dist/test.js",
  "repository": "https://github.com/banou26/steins-gate",
  "author": "Banou",
  "license": "MIT",
  "scripts": {
    "index": "parcel src/index.ts --target node --public-url ./",
    "test": "parcel src/test/index.ts --out-file test.js --no-hmr --public-url ./",
    "browser-runner": "parcel src/browser-runner/index.html --out-file browser-runner.html --no-hmr --public-url ./",
    "dev": "concurrently \"yarn run index\" \"yarn run test\""
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "babel-plugin-espower": "^3.0.0",
    "babylon-walk": "^1.0.2",
    "chalk": "^2.4.1",
    "fast-glob": "^2.2.4",
    "flatted": "^2.0.0",
    "koa": "^2.6.2",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "ora": "^3.0.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-babel-typescript": "^0.0.3",
    "perf_hooks": "^0.0.1",
    "power-assert": "^1.6.1",
    "puppeteer-firefox": "^0.4.2",
    "rxjs": "6",
    "strip-ansi": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@types/puppeteer": "^1.11.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "puppeteer": "^1.13.0",
    "standard": "^12.0.1",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">= 11.0.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
